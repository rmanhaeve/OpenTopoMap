<Style name="hillshade-5000">
	<Rule>
		&maxscale_zoom1;
		&minscale_zoom4;
		<RasterSymbolizer comp-op="grain-merge" opacity="0.9" scaling="bilinear" />
	</Rule>
</Style>
<Layer name="hillshade-5000" srs="+proj=lcc +lat_0=50.797815 +lon_0=4.35921583333333 +lat_1=49.8333333333333 +lat_2=51.1666666666667 +x_0=649328 +y_0=665262 +ellps=GRS80 +towgs84=0,0,0,0,0,0,0 +units=m +no_defs +type=crs">
	<StyleName>hillshade-5000</StyleName>
	<Datasource>
		<Parameter name="type">gdal</Parameter>
		<Parameter name="file">dem/hillshade-5000.tif</Parameter>
		<Parameter name="format">tiff</Parameter>
	</Datasource>
</Layer>

